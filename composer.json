{
    "name": "webleit/zohocrmapi",
    "description": "Zoho CRM API - PHP SDK",
    "license": "MIT",
    "keywords": ["zoho", "crm", "zohocrm", "api"],
    "authors": [
        {
            "name": "Daniele Rosario",
            "email": "daniele@weble.it"
        }
    ],
    "require": {
        "php" : ">=7.2.0",
        "guzzlehttp/guzzle": "^6.3||^7.0",
        "illuminate/collections": "^8.0",
        "psr/cache": "^1.0",
        "doctrine/inflector": "^2",
        "bentools/guzzle-throttle-middleware": "^0.4.1",
        "weble/zohoclient": "^4.0.0",
        "spatie/enum": "^2.3",
        "cache/filesystem-adapter": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "nunomaduro/collision": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Webleit\\ZohoCrmApi\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
