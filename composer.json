{
    "name": "webleit/zohocrmapi",
    "description": "Zoho CRM API - PHP SDK",
    "license": "MIT",
    "keywords": ["zoho", "crm", "zohocrm", "api"],
    "authors": [
        {
            "name": "Daniele Rosario",
            "email": "daniele@weble.it"
        }
    ],
    "require": {
        "php" : "^7.3 || ^8.0",
        "guzzlehttp/guzzle": "^6.3||^7.0",
        "illuminate/collections": "^8.0",
        "psr/cache": "^1.0",
        "doctrine/inflector": "^2.0",
        "weble/zohoclient": "^4.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "nunomaduro/collision": "^5.0",
        "cache/filesystem-adapter": "^1.0",
        "caseyamcl/guzzle_retry_middleware": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Webleit\\ZohoCrmApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webleit\\ZohoCrmApi\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
